{
  "name": "game",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prestart": "npx -y kill-port --port 5173",
    "deps:install": "npm install --install-strategy=nested --legacy-peer-deps",
    "deps:reinstall": "rm -r node_modules && rm package-lock.json && rm -r dist && npm run deps:install",
    "dev": "npm run prestart && netlify dev",
    "dev:bot-api": "dotenv -c development -e ./.env -- zsh -c 'telegram-bot-api --local --http-port=$BOT_API_PORT --api-id=$BOT_API_ID --api-hash=$BOT_API_HASH --verbosity=$BOT_API_VERBOSITY --dir ./.telegram-bot-api'",
    "build": "netlify build",
    "preview": "vite preview",
    "start": "netlify build && vite preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@netlify/functions": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "node-stdlib-browser": "^1.3.1",
    "qs": "^6.14.0",
    "serverless-http": "^3.2.0",
    "telegraf": "^4.16.3",
    "three": "^0.171.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/three": "^0.171.0",
    "dotenv-cli": "^8.0.0",
    "telegram-webapps": "^8.0.3",
    "typescript": "^4.9.5",
    "vite": "^6.0.3"
  }
